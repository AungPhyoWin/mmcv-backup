@model MyanmarCV.Models.pdfCreation

@{
    ViewBag.Title = "PersonalDetail";
    Layout = "~/Views/Shared/_pdfLayout.cshtml";
}
   <div style="padding:0px 0px 0px 0px; height:950px;">
    <table cellpadding="0" cellspacing="5px" border="0" style=" background-color:#ffffff; width:650px;" class="pdfText">
       <tr>
            <td align="center" colspan="2" style="color:#cccccc;">
            <br />
            Resume of  @Html.DisplayFor(model => model.name)
            <br />
            <br />
            <br />
            </td>
       </tr>
    </table>
    <table cellpadding="0" cellspacing="5px" border="0" style=" background-color:#ffffff; width:650px;" class="pdfText2">
     <tr>
        <td colspan="2">
            <div style="background-color:#ededed; color:#454343; font-family:Arial; font-weight:bold; height:20px; width:650px; text-align:left; padding-left:5px; padding-top:5px;">
                WORKING EXPERIENCE
            </div>
        </td>
    </tr>
     <tr>
        <td>
            <div style="color:#696868">latestPosition</div>
   
        </td>
        <td>
             <div >
                @Html.DisplayFor(model => model.latestPosition)
            </div>
        </td>
    </tr>
     <tr>
        <td>
          <div style="color:#696868">Years Of Working Experience</div>
        </td>
        <td>
              <div >
                @Html.DisplayFor(model => model.yearOfWorkingExp)
              </div>
        </td>
    </tr>
     <tr>
        <td colspan="2">
            <div style="background-color:#ededed; color:#454343; font-family:Arial; font-weight:bold; height:20px; width:650px; text-align:left; padding-left:5px; padding-top:5px;">
            OTHER SKILL
            </div>
            
        </td>
    </tr>
    <tr>
        <td>
            <div style="color:#696868">Other Skill</div>
       </td>
        <td>
             <div >
                @Html.DisplayFor(model => model.otherSkill)
            </div>
        </td>
    </tr>
    <tr>
        <td>
             <div style="color:#696868">Language</div>
  
        </td>
        <td>
              <div >
            @Html.DisplayFor(model => model.language)
            </div>
        </td>
    </tr>
    <tr>
        <td>
           <div style="color:#696868">Certificate</div>
        </td>
        <td>
             <div>
                    @Html.DisplayFor(model => model.certificate)
            </div>
        </td>
    </tr>
    </table>
   </div>
    

    
    

    
   

    
    

    
    
   
   
  

    
   

  
    
    

    
   

    
    
    
    
   

    
    

    
   

   

    
    
  
  

   
    
  

    

    
   
   
  

    

    
